{
  "type": "object",
  "title": "extractor configuration",
  "properties": {
    "authorization": {
      "type": "object",
      "title": "Authorization",
      "propertyOrder": 1,
      "properties": {
    "email": {
      "type": "string",
      "title": "Email",
      "propertyOrder": 1
    },
    "#password": {
      "type": "string",
      "format": "password",
      "title": "Password",
      "propertyOrder": 2
    },
    "company_id": {
      "type": "string",
      "title": "Company ID",
      "propertyOrder": 3
    }},
   "required": ["email", "#password"]
    },
    "time-range": {
      "type": "object",
      "title": "Time range settings",
      "propertyOrder": 2,
      "properties": {
    "from": {
      "type": "string",
      "title": "From",
      "description": "Enter timestamp in `%Y-%m-%dT%H:%M:%S` format or a relative date of valid dateparser format. Defaults to now if empty.",
      "propertyOrder": 1
    },
    "to": {
      "type": "string",
      "title": "To",
      "description": "Enter timestamp in `%Y-%m-%dT%H:%M:%S` format or a relative date of valid dateparser format. Defaults to now if empty.",
      "propertyOrder": 2
      }
    }},
    "endpoints": {
      "type": "object",
      "title": "Endpoints",
      "description": "It is necessary to use users endpoint for endpoints worklog, edit-time and timeuse",
      "properties": {
        "_users": {
          "type": "boolean",
          "title": "users",
          "format": "checkbox",
          "propertyOrder": 10
        },
        "worklog": {
          "type": "boolean",
          "title": "worklog",
          "format": "checkbox",
          "options": {
            "dependencies": {
              "_users": true
            }
          },
          "propertyOrder": 20
        },
        "edit-time": {
          "type": "boolean",
          "title": "edit-time",
          "format": "checkbox",
          "options": {
            "dependencies": {
              "_users": true
            }
          },
          "propertyOrder": 30
        },
        "timeuse": {
          "type": "boolean",
          "title": "timeuse",
          "format": "checkbox",
          "options": {
            "dependencies": {
              "_users": true
            }
          },
          "propertyOrder": 40
        },
        "projects": {
          "type": "boolean",
          "title": "projects",
          "format": "checkbox",
          "propertyOrder": 50
        },
        "tasks": {
          "type": "boolean",
          "title": "tasks",
          "format": "checkbox",
          "propertyOrder": 60
        }
      },
      "propertyOrder": 7
    },
      "increment": {
      "type": "boolean",
      "title": "Incremental load",
        "description": "If set to false, component will truncate data in existing Keboola tables.",
        "default": true,
      "propertyOrder": 8
    }
  }
}